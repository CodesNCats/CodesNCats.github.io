    <GalleryStatus textFormat="standard_white" autoSize="true"/>
    <GotoBox inputTextFormat="standard_white" statusTextFormat="standard_white" backgroundColor="#000000" backgroundAlpha="30" borderWidth="1" borderColor="#FF6600" cornerRadius="3" innerPadding="5" inputWidth="20" inputPadding="2" disableAutoplayOnClick="true"/>
