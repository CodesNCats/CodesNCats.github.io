    <ThumbBarScrollBackButton shadowColor="#333333" arrowSpacing="2"/>
    <ThumbBarScrollForwardButton shadowColor="#333333" arrowSpacing="2"/>
    <ThumbBarStatus textFormat="title_text"/>
    <AudioController transparentRollover="true" effectDuration="800" effectHiddenState="left" backgroundColor="#000000" backgroundAlpha="70" cornerRadius="5" borderWidth="0" borderColor="#3333FF"/>
