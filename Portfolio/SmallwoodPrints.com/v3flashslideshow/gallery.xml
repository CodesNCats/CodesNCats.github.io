    <ListBarScrollBackButton shadowColor="#FFFFFF" arrowSpacing="2"/>
    <ListBarScrollForwardButton shadowColor="#FFFFFF" arrowSpacing="2"/>
    <ListBarStatus textFormat="standard_white"/>
    <ThumbBarModule innerPadding="5" scrollBarSize="8"/>
