<?xml version="1.0" encoding="utf-8"?><slideshow version="3"><styles>
    <Background backgroundColor="#FFFFFF" backgroundAlpha="100" bevelColor="#FFFFFF" bevelStrength="0" borderWidth="1" borderColor="#FFFFFF"/>
