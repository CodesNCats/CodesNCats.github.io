    <ControlButtonDefaults backgroundColorDisabled="#333333" backgroundColorEnabled="#333333" backgroundColorSelected="#333333" shadowColor="#333333" shadowAlpha="100"/>
    <UIButton backgroundColorDisabled="#333333" borderColorDisabled="#333333" bevelDisabled="0" backgroundColorEnabled="#333333" borderColorEnabled="#333333" bevelEnabled="50" backgroundColorSelected="#333333" borderColorSelected="#333333" bevelSelected="50" textFormat="standard_dark" borderWidth="1" cornerRadius="4"/>
