    <JumpBarScrollForwardButton shadowColor="#999999" arrowSpacing="2"/>
    <ListBar backgroundColor="#333333" backgroundAlpha="100" cornerRadius="3" effectSpeed="1000" labelType="title" disableAutoplayOnClick="true"/>
